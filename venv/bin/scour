#!/home/neno/PycharmProjects/learn/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'scour==0.37','console_scripts','scour'
__requires__ = 'scour==0.37'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('scour==0.37', 'console_scripts', 'scour')()
    )
